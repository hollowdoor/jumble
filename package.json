{
    "name": "jumble",
    "version": "0.0.7",
    "preferGlobal": "true",
    "engines": { "node": "*" },
    "description": "Build apps with browserify.",
    "author": "Quentin Engles",
    "dependencies": {
        "browserify": "9.*",
        "uglifyify": "3.*",
        "yargs": "3.*",
        "chokidar": "1.*",
        "graceful-fs": "3.*",
        "cheerio": "0.*",
        "ncp": "2.*",
        "rimraf": "2.*",
        "babelify": "6.*",
        "mkdirp": "0.*",
        "glob": "5.*",
        "clean-css": "3.*",
        "syntax-error": "1.*",
        "mime": "1.*",
        "tmp": "0.*",
        "derequire": "2.*"
    },
    "bin": {
        "jumble": "./index.js"
    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/hollowdoor/jumble.git"
    },
    "license": "MIT",
    "keywords": [
        "build",
        "browserify",
        "task",
        "modules",
        "app",
        "apps",
        "html5",
        "es6",
        "babel"
    ]
}
